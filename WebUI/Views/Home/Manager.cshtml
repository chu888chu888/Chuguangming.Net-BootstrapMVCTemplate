<style type="text/css">
    .waterfallcontainer {
        -webkit-column-width: 230px;
        //webkit内核浏览器，列与列之间的宽度 -moz-column-width:230px;
        -o-colum-width: 230px;
        -webkit-column-gap: 10px;
        //webkit内核浏览器，列与列之间的缝隙 -moz-column-gap:10px;
        -o-column-gap: 10px;
        text-align: center;
        width: auto;
    }

        .waterfallcontainer div {
            border-radius: 5px;
            //圆角 background:#f5f5f5;
            border: #ccc 1px solid;
            display: inline-block;
            width: auto;
            position: relative;
            text-align: center;
            margin-bottom: 10px;
        }

            .waterfallcontainer div img {
                margin: 10px 10px 0 10px;
            }

            .waterfallcontainer div p {
                padding: 0px 10px 0px 10px;
                margin: 0px;
                line-height: 25px;
                text-align: left;
                font-size: 12px;
            }

    .actions {
        left: 0;
        position: absolute;
        top: 5px;
        width: 100%;
        z-index: 3;
        display: none;
    }
</style>
